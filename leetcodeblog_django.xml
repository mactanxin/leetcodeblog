<uwsgi>
    <socket>127.0.0.1:8089</socket>
    <chdir>/opt/app/python/leetcodeblog</chdir>
    <master/>
    <processes>4</processes>
    <post-buffering>25000</post-buffering>
    <pythonpath>..</pythonpath>
    <touch-reload>/opt/app/python/leetcodeblog</touch-reload>
    <daemonize>/opt/logs/uwsgi.log</daemonize>
    <module>uwsgi</module>
</uwsgi>
